<%- include("partials/header") -%>
    <h1>Home</h1>
    <p><%=homeContent %></p>
    
    <% posts.forEach(function(post){ %>
        <h1> <%= post.title %> </h1>
        <p> <%= post.content.substring(0,100)+"..." %>
        <% var url = "/posts/"+post.title %>
        <% url = url.replace(/ /g,"%20"); %>
        <a href=<%=url%> >Read More</a></p>
    <% }) %>
<%- include("partials/footer") -%>